{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.acme.com/excel/analysis-result/1.0.0",
  "type": "object",
  "title": "Excel Analysis Result",
  "description": "Result from Excel Analysis Crew",
  "additionalProperties": false,
  "properties": {
    "@context": {
      "type": "object",
      "description": "JSON-LD context for semantic meaning"
    },
    "request_id": {
      "type": "string",
      "description": "ID of the request this responds to"
    },
    "schema_analysis": {
      "type": "object",
      "description": "Schema and structure analysis",
      "properties": {
        "columns": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "type": { "type": "string" },
              "ontology_mapping": { "type": "string" },
              "sample_values": { "type": "array" }
            }
          }
        },
        "row_count": { "type": "integer" },
        "sheets": { "type": "array", "items": { "type": "string" } }
      }
    },
    "data_results": {
      "type": "object",
      "description": "Query results",
      "properties": {
        "rows": { "type": "array" },
        "row_count": { "type": "integer" },
        "columns": { "type": "array", "items": { "type": "string" } }
      }
    },
    "insights": {
      "type": "object",
      "description": "Generated insights",
      "properties": {
        "summary": { "type": "string" },
        "trends": { "type": "array", "items": { "type": "string" } },
        "anomalies": { "type": "array", "items": { "type": "string" } },
        "recommendations": { "type": "array", "items": { "type": "string" } }
      }
    },
    "answer": {
      "type": "string",
      "description": "Natural language answer to the query"
    },
    "confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Confidence in the analysis"
    },
    "execution_time_ms": {
      "type": "integer",
      "description": "Analysis execution time"
    },
    "agents_involved": {
      "type": "array",
      "items": { "type": "string" },
      "description": "Agents that contributed to the analysis"
    }
  },
  "required": [
    "@context",
    "request_id",
    "answer",
    "confidence"
  ]
}
